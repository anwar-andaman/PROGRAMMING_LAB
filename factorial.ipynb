   "source": [
    "#Program to find factorial \n",
    "num = int(input(\"Enter an integer number:\"))\n",
    "fact = 1\n",
    "for i in range(1,num+1):\n",
    "    fact = fact*i\n",
    "print(\"The factorial of number\",num,\"is\",fact)"
