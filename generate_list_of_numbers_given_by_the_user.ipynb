    "#Progam to generate a list of numbers given by the user\n",
    "list_string = input(\"Enter the numbers separated by commas: \")\n",
    "li = list_string.split(\",\")\n",
    "n = 0\n",
    "for i in li:\n",
    "    li[n]=int(i)\n",
    "    n=n+1\n",
    "print(li)"
